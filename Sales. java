public class _3D_Array {
public static void main(String[] args) {
int sum=0;
int[][][]sales={
    {
    {10,20,30},
    {15,25,35}
},
   {
    {5,10,15},
    {8,16,24}
}
           };
for(int i=0;i<2;i++){
for(int j=0;j<2;j++){
for(int k=0;k<3;k++){
System.out.print(sales[i][j][k]+" ");
}
System.out.println();
    }
}
for(int i=0;i<1;i++){
 sum+=sales[0][1][2];  
}
System.out.println("Sum="+sum);
}
}