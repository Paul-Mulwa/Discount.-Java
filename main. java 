import java.util.Scanner;
public class Main {
public static void main(String[] args) {
Scanner input=new Scanner(System.in);

int n,sum=0;
System.out.println("Enter the value of n");
n=input.nextInt(); 

System.out.println("Numbers from 1 to " + n + ":");
for(int i=1;i<=n;i++){
 System.out.print(i); 
 sum+=i;
 System.out.println("Sum="+sum);
}


    }
}